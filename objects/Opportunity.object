        <errorConditionFormula>/* Display Error if Condition is Met */ 
AND( 

/* Cond1 – If user changed state or other field when stage was closed */
 OR(
 AND(NOT(IsChanged(StageName)),OR( ISPICKVAL(StageName, &quot;Closed Won&quot;),ISPICKVAL(StageName, &quot;Closed Lost&quot;))),

 AND(ISCHANGED(StageName),
               OR(TEXT(PRIORVALUE(StageName)) =  &apos;Closed Lost&apos;, TEXT(PRIORVALUE(StageName)) = &apos;Closed Won&apos;
                ) /* End of AND */
             ) /* End of  AND */
 ) /* End of OR */
,
/* Cond2 - If user is not administrator */
      $Profile.Name&lt;&gt;&apos;System Administrator&apos;,

/* If &lt; 10 days from closed date, owner can change description or if &gt; 10 days lock all fields */
        OR(ISCHANGED( Name ) , ISCHANGED(  AccountId  ), ISCHANGED(   Type   ), ISCHANGED(     CampaignId     ), ISCHANGED(    ForecastCategoryName    ), ISCHANGED(    CloseDate    ), ISCHANGED(    StageName    ),ISCHANGED(    Probability    ),ISCHANGED(     Amount     ),ISCHANGED(    LeadSource    ),ISCHANGED(    NextStep    ),
 AND(ISCHANGED(Description), ($User.Id&lt;&gt;OwnerId   &amp;&amp;  $User.Id&lt;&gt;  Owner.ManagerId )),
 AND(ISCHANGED(Description), CloseDate + 10 &lt; Today())
         ) /* End of OR */

) /* End of AND */</errorConditionFormula>
        <errorMessage>Opportunity cannot be edited after it is Closed.  Refer to the opportunity owner or the system administrator in order to make a change.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Markdown_LT_1000</fullName>
        <active>true</active>
        <errorConditionFormula>PT_Markdown__c &gt; 1000</errorConditionFormula>
        <errorDisplayField>PT_Markdown__c</errorDisplayField>
        <errorMessage>Markdown cannot be greater than 1000</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Prevent_Line_Deletion</fullName>
        <active>false</active>
        <description>Prevents users from deleting opportunity products after an opportunity is closed. Create the following validation rule example on opportunities. It uses a custom roll-up summary field on opportunities that counts the number of   opportunity products</description>
        <errorConditionFormula>AND(OR(ISPICKVAL(StageName , &quot;Closed Won&quot;),
ISPICKVAL(StageName , &quot;Closed Lost&quot;)),
Line_Item_Count__c  &lt; PRIORVALUE(Line_Item_Count__c))</errorConditionFormula>
        <errorMessage>Opportunity Line Items cannot be deleted because the Opportunity is closed.</errorMessage>
    </validationRules>
</CustomObject>
