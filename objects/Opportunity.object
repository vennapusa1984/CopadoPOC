    <validationRules>
        <fullName>Ex1_Opportunity_Lock_after_Close</fullName>
        <active>false</active>
        <description>Prevent editing when opportunity closes</description>
        <errorConditionFormula>/* Display Error if Condition is Met */ 
AND( 

/* Cond1 – If user changed state or other field when stage was closed */
 OR(
 AND(NOT(IsChanged(StageName)),OR( ISPICKVAL(StageName, &quot;Closed Won&quot;),ISPICKVAL(StageName, &quot;Closed Lost&quot;))),

 AND(ISCHANGED(StageName),
               OR(TEXT(PRIORVALUE(StageName)) =  &apos;Closed Lost&apos;, TEXT(PRIORVALUE(StageName)) = &apos;Closed Won&apos;
                ) /* End of AND */
             ) /* End of  AND */
 ) /* End of OR */
,
/* Cond2 - If user is not administrator */
      $Profile.Name&lt;&gt;&apos;System Administrator&apos;,

/* If &lt; 10 days from closed date, owner can change description or if &gt; 10 days lock all fields */
        OR(ISCHANGED( Name ) , ISCHANGED(  AccountId  ), ISCHANGED(   Type   ), ISCHANGED(     CampaignId     ), ISCHANGED(    ForecastCategoryName    ), ISCHANGED(    CloseDate    ), ISCHANGED(    StageName    ),ISCHANGED(    Probability    ),ISCHANGED(    LeadSource    ),ISCHANGED(    NextStep    ),
 AND(ISCHANGED(Description), $User.Id&lt;&gt;OwnerId),
 AND(ISCHANGED(Description), $User.Id=OwnerId, CloseDate + 10 &lt; Today())
         ) /* End of OR */

) /* End of AND */</errorConditionFormula>
        <errorMessage>Opportunity cannot be edited after it is Closed.  Refer to the opportunity owner or the system administrator in order to make a change.</errorMessage>
    </validationRules>
